[receive]
	requireChangeId = true
[submit]
	action = cherry pick
[project]
[access "refs/*"]
	pushMerge = group Administrators
	pushMerge = group TYPO3CMS/Extensions/crawler-Leaders
	owner = group TYPO3CMS/Extensions/crawler-Leaders
	push = +force group TYPO3CMS/Extensions/crawler-Leaders
[access "refs/for/refs/heads/*"]
	forgeCommitter = group TYPO3CMS/Extensions/crawler-Leaders
	pushMerge = group TYPO3CMS/Extensions/crawler-Leaders
	pushMerge = group TYPO3CMS/Extensions/crawler-Members
[access "refs/heads/*"]
	create = group TYPO3CMS/Extensions/crawler-Leaders
	label-Code-Review = -2..+2 group TYPO3CMS/Extensions/crawler-Leaders
	label-Code-Review = -2..+2 group TYPO3CMS/Extensions/crawler-Members
	label-Verified = -1..+2 group TYPO3CMS/Extensions/crawler-Leaders
	label-Verified = -1..+2 group TYPO3CMS/Extensions/crawler-Members
	submit = group TYPO3CMS/Extensions/crawler-Leaders
	submit = group TYPO3CMS/Extensions/crawler-Members
[access "refs/tags/*"]
	create = group TYPO3CMS/Extensions/crawler-Leaders
	pushTag = group TYPO3CMS/Extensions/crawler-Leaders
	push = +force group TYPO3CMS/Extensions/crawler-Leaders
